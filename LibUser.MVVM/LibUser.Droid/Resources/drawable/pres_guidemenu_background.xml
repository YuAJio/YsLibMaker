<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape>
            <solid android:color="#6697FFFF" />
            <stroke android:width="1dp" android:color="#FF97FFFF" />
            <corners android:radius="10dp" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <solid android:color="#B197FFFF" />
            <stroke android:width="1dp" android:color="#AAFEFE" />
            <corners android:radius="10dp" />
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="#6697FFFF" />
            <stroke android:width="1dp" android:color="#FF97FFFF" />
            <corners android:radius="10dp" />
        </shape>
    </item>
</selector>